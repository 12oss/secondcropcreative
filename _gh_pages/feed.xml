---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/css" href="/assets/css/rssfeed.css" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>Second Crop Creative Blog RSS</title>
		<description>Latest blog posts from Second Crop Creative</description>

		<link>{{ site.url }}/blog</link>
		{% for post in site.posts %} {% unless post.draft %}
		<item>
			<title><![CDATA[{{ post.title }}]]></title>
			<description><![CDATA[{{ post.excerpt }}]]></description>
			<description type="html"><![CDATA[<img src="{{ site.url }}{{ post.image }}"/>]]></description>
			<pubDate>{{ post.date | date_to_rfc822}}</pubDate>
			<link>{{ post.url | prepend: site.url }}</link>
			<guid isPermaLink="true">{{ post.url | prepend: site.url }}</guid>
		</item>
		{% endunless %} {% endfor %}
	</channel>
</rss>
